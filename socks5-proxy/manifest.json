{
  "manifest_version": 2,
  "name": "SOCKS5 Proxy",
  "version": "1.0.0",
  "description": "Configures a SOCKS5 proxy for the browser.",
  "permissions": [
    "proxy",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}